---
title: GoCD Events
---

<div class="inner-banner plugins">
  <div class="container">
    <h1 class="caption"><span>Events</span></h1>
  </div>
</div>

<main>
  <div class="container">

    <div class="webinars">
      <h3 class="page-headings">Webinars and Demos</h3>
      <ul class="webinars-list">
        <% data.webinars.each do |webinar| %>
        <li>
          <div class="box">
            <div class="datebox">
              <span class="month"><%= webinar.month %></span>
              <span class="date"><%= webinar.day %></span>
            </div>
          </div>
          <div class="details">
            <a href="<%= webinar.url %>" class="title"><%= webinar.title %></a>
            <span class="time"><%= webinar.date %></span>
            <p><%= webinar.description %></p>
          </div>
        </li>
        <% end %>
      </ul>
    </div>

  </div>
</main>
